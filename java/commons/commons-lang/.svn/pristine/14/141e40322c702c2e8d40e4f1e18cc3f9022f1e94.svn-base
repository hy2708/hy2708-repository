	@Test
	public void name() throws UnsupportedEncodingException {
		StringBuilder stringBuilder = new StringBuilder();
		for (int i = 0; i < 100; i++) {
			stringBuilder.append("abcde");
		}
		byte[] b = stringBuilder.toString().getBytes("utf-8");
		System.err.println("原"+stringBuilder.toString().length()+"  "+b.length);
		String string = new String(b);
		byte[] b2 = string.getBytes("utf-8");
		System.err.println("后"+string.length()+"  "+b2.length);
		
	}
	
